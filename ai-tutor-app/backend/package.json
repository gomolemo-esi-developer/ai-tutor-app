{
  "name": "tutorverse-backend",
  "version": "1.0.0",
  "description": "TutorVerse Backend - Lambda-based serverless API",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/app.ts",
    "dev:local": "sam local start-api",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy": "npm run build && node scripts/deploy-lambdas.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "seed": "ts-node scripts/seed-dynamodb.ts",
    "seed:dynamodb": "ts-node scripts/seed-dynamodb.ts",
    "clear-chats": "ts-node scripts/clear-all-chats-no-confirm.ts"
  },
  "keywords": [
    "aws",
    "lambda",
    "dynamodb",
    "typescript"
  ],
  "author": "TutorVerse Team",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.122",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.8.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.433.0",
    "@aws-sdk/client-dynamodb": "^3.433.0",
    "@aws-sdk/client-s3": "^3.433.0",
    "@aws-sdk/client-ses": "^3.971.0",
    "@aws-sdk/lib-dynamodb": "^3.433.0",
    "@aws-sdk/s3-request-presigner": "^3.433.0",
    "archiver": "^7.0.1",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "qrcode": "^1.5.4",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  }
}
